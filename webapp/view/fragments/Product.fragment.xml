<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core">
    <SelectDialog
        noDataText="No Product Found"
        title="Select Product"
        search="handleSearchProd"
        confirm="handleProductDialogClose"
        items="{ProductListModel>/}" >
        <StandardListItem
            
            title="{ProductListModel>PrdctName}"
            description="{ProductListModel>Prdct}"
            type="Active"/>
    </SelectDialog>
</core:FragmentDefinition>

