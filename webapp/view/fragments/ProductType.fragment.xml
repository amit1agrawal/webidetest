<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core">
    <SelectDialog
        noDataText="No Product Found"
        title="Select Product"
        search="handleSearchProdType"
        confirm="handleProductTypeDialogClose"
        items="{ProductTypeListModel>/}" >
        <StandardListItem
            title="{ProductTypeListModel>PrdctTypeDesc}"
            description="{ProductTypeListModel>PrdctType}"
            type="Active"/>
    </SelectDialog>
</core:FragmentDefinition>