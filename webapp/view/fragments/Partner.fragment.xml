<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core">
    <SelectDialog
        noDataText="No Partner Found"
        title="Select Partner"
        search="handleSearchPartner"
        confirm="handlePartnerDialogClose"
        items="{PartnerListModel>/}" >
        <StandardListItem
            title="{PartnerListModel>PrtnrName}" 
             description="{PartnerListModel>Prtnr}"
             type="Active"/>
    </SelectDialog>
</core:FragmentDefinition>